REPOPICK[0]=''											# Space separeted list of gerrit change ID's which will be cherrypicked
SKIP_SYNC_TIME=''										# if synced in the last given seconds set skip sync true (if empty or not a number "1800" is used)
SYNC_PARAMS=''											# Override syn parameters (Default: '-q -d -j100' )
DINNER_USE_CCACHE='1'									# Use ccache or not 1/0 (Default: 1)
DINNER_CCACHE_DIR="${CCACHE_DIR:-"${HOME}/.ccache"}"	# Change ccache directory (Default: ${CCACHE_DIR:-"$HOME/.ccache"})
DINNER_CCACHE_SIZE=''									# Change ccache max cache size in gigabytes recomended are 25G (Default: ccache default=1G )
